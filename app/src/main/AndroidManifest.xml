<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.app.main">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExercicioRetrofit">
        <activity
            android:name=".exercicioroom.views.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".exercicioretrofit.view.RetrofitActivity" />
        <activity android:name=".exercicioroom.views.UserActivity" />
        <activity
            android:name=".exercicioroom.views.NewNoteActivity"
            android:label="Criar nota"
            android:parentActivityName=".exercicioroom.views.ListNotesActivity" />
        <activity
            android:name=".exercicioroom.views.ListNotesActivity"
            android:label="Todas as notas" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>